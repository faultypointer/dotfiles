  wallpaper = "tshd.jpg";

  font = "SFProDisplay Nerd Font";
  font-mono = "FiraCode Nerd Font Mono";
  font-size = 12;

  rounding = 10;
  gaps-in = 2;
  gaps-out = 2 * 2;
  active-opacity = 0.95;
  inactive-opacity = 0.9;
  blur = true;
  border-size = 1;
  animation-speed = "fast"; # "fast" | "medium" | "slow"
  fetch = "nerdfetch"; # "nerdfetch" | "neofetch" | "pfetch" | "none"

  bar = {
    transparent = true;
    floating = true;
    font-size = 12;
  };

